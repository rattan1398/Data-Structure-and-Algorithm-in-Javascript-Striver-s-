function insertionSort(array){
    for(let i=0; i<=array.length-1; i++){ //iterate till the length of the array.
        let j=i; //set the value of j to i;
        while(j>0 && array[j-1]>array[j]){ //till j>= and element at j-1 index is greater than element at j 
            [array[j-1],array[j]] = [array[j],array[j-1]]; //swap them
            j--; //decrement the value of j
        }
    }return array;
}
