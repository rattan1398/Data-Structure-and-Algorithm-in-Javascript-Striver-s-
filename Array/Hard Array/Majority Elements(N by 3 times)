//Find the elements that appears more than array.length/3 times in the array

function majorityElements(array){
    let freq = {};
    let maxFrequent = -Infinity;
    let maxElement;

    for(let i=0; i<array.length; i++){
        freq[array[i]] = (freq[array[i]] || 0)+1;

        for(let j in freq){
            if(freq[j]>maxFrequent){
                maxFrequent = freq[j]; //How many times does the element occurred
                maxElement = j; //Element that occured the most
            }
        }
    }
    let n = Math.floor(array.length/3); //If the element occurs more than 'n' times in array then it is majority element
    if(maxFrequent>=n){
        return maxElement;
    }else{
        return ('No majority element');
    }
}
